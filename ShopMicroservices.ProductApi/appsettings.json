{
  "DatabaseSettings": {
    "DatabaseName": "service_productApi_database",
    "CollectionName": "catalogo",
    "ConnectionString": "mongodb://localhost:27017"
  },
  "MassTransitSettings": {
    "RabbitMQHost": "localhost"
  },
  "ServiceSettings": {
    "ServiceName": "shopmicroservices.productapi",
    "ServiceHost": "shopmicroservices.productapi",
    "ServicePort": 8080,
    "ServiceDiscoveryAddress": "http://localhost:8500",
    "HealthCheckUrl": "http://localhost:8080/api/v1/product/health",
    "HealthCheckIntervalSeconds": 10,
    "HealthCheckTimeoutSeconds": 5
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
